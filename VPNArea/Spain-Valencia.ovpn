client
proto udp
dev tun0
remote 185.140.114.127 111
resolv-retry infinite
nobind
persist-key
persist-tun
ns-cert-type server
ca "ca.crt"
verb 3
auth-user-pass
cipher AES-256-CBC
auth SHA256
mute-replay-warnings
passtos
tun-mtu 1500
script-security 2
up /etc/openvpn/update-resolv-conf
down /etc/openvpn/update-resolv-conf
